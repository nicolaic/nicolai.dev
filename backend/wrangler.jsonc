{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "compatibility_date": "2025-04-17",
  "main": "./src/index.ts",
  "name": "nicolai-dev-backend",
  "route": "nicolai.dev/api/*",
  "workers_dev": false,
  "preview_urls": false,
  "observability": {
    "enabled": true
  },
  "vars": {
    "ENVIRONMENT": "dev"
  },
  "env": {
    "production": {
      "vars": {
        "ENVIRONMENT": "production"
      }
    }
  }
}
