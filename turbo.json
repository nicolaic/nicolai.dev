{
	"$schema": "./node_modules/turbo/schema.json",
	"tasks": {
		"test": {
			"dependsOn": ["^test"]
		},
		"build": {
			"outputs": ["dist/**"]
		},
		"deploy": {
			"dependsOn": ["^deploy"],
			"passThroughEnv": ["CLOUDFLARE_ACCOUNT_ID", "CLOUDFLARE_API_TOKEN"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
